<Window x:Class="NFRV.Configuration"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:NFRV"
        mc:Ignorable="d"
        Title="Configuration" Height="173.2" Width="387.2">
    <Grid Margin="0,0,0,0">
        <Button x:Name="saveBtn" Content="Save" HorizontalAlignment="Left" Margin="296,111,0,0" VerticalAlignment="Top" Width="75" Height="22" RenderTransformOrigin="0.5,0.5" Click="saveBtn_Click"/>
        <Button x:Name="cancelBtn" Content="Cancel" HorizontalAlignment="Left" Margin="216,111,0,0" VerticalAlignment="Top" Width="75" Height="22" RenderTransformOrigin="0.5,0.5" Click="cancelBtn_Click"/>
        <Label Content="Web server( example: http://192.168.0.102:5000/ocr )" HorizontalAlignment="Left" Margin="10,20,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="webServerTxtBox" HorizontalAlignment="Left" Height="24" Margin="10,46,0,0" TextWrapping="Wrap" Text="http://" VerticalAlignment="Top" Width="361"/>

    </Grid>
</Window>
